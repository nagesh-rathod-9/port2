<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />
    <link
      href="https://getbootstrap.com/docs/5.3/assets/css/docs.css"
      rel="stylesheet"
    />
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">

    <title>Bootstrap Example</title>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    <script
      src="https://kit.fontawesome.com/0ca23af288.js"
      crossorigin="anonymous"
    ></script>
    <link rel="stylesheet" href="css/style.css" />
    <script src="https://code.jquery.com/jquery-3.7.1.js"></script>
    <link rel="stylesheet" href="css/Rstyle.css" />
    <style>
      @import url("https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap");
    </style>
    <style>
      @import url("https://fonts.googleapis.com/css2?family=Bebas+Neue&family=Luckiest+Guy&display=swap");
    </style>
    <style>
      @import url("https://fonts.googleapis.com/css2?family=Bebas+Neue&family=Luckiest+Guy&family=Patrick+Hand&display=swap");
    </style>
    <style>
      @import url('https://fonts.googleapis.com/css2?family=Instrument+Serif&display=swap');
    </style>
    <style>
      @import url('https://fonts.googleapis.com/css2?family=Alegreya+Sans+SC&family=Instrument+Serif&display=swap');
    </style>
    <style>
      @import url('https://fonts.googleapis.com/css2?family=Josefin+Sans&display=swap');
    </style>
    <style>
      @import url('https://fonts.googleapis.com/css2?family=Josefin+Sans&display=swap');
    </style>
  </head>
  <body>
    <!-- navbar -->
    <nav class="navbar navbar-expand-lg" style="position: sticky; top: 0; z-index: 999; background-color: #dadada;">
      <div class="container-fluid">
        <a class="navbar-brand" href="#"><img src="image/logo.jpg" alt="" /></a>
        <button
          class="navbar-toggler shadow-none border-0"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarNav"
          aria-controls="navbarNav"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav" id="ul">
            <li class="nav-item">
              <a class="nav-link active" aria-current="page" href="/"
                >Home</a
              >
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/about" style="background-color: black !important; color: white !important;">About</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/all">Portfolio</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/contact">Contact</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
    <!-- navbar -->
  </body>
  <section id="home">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-4 my_i myimg mt-5">
          <img src="<%=home_data[0].image1%>" alt="" class="mt-4"/>
        </div>
        <div class="col-md-4 my_i myimg2">
          <img src="image/nagu2.jpg" alt="" />
        </div>
        <div class="col-md-7 my_about pb-5">
          <h3 class="about_me">ABOUT ME</h3>
          <hr>
          <div class="row" >
          <div class="col-md-5" data-aos="fade-up">
            <p>
                <%=about_detail[0].disc_1%></p>
                <p><%=about_detail[0].disc_2%></p>
          </div>
          <div class="col-md-7 names" data-aos="fade-up">
            <p class="bio"><b class="text-secondary">Name :</b> <span><%=about_detail[0].name%></span><br></p>
            <p class="bio_1"> <b>Address :</b><span><%=about_detail[0].address%></span><br></p>
            <p class="bio_1"><b>Study :</b><span><%=about_detail[0].study%></span><br> </p> 
            <p class="bio_1"><b>Degree :</b><span><%=about_detail[0].degree%></span><br> </p> 
            <p class="bio_1"><b>Mail :</b><span><%=about_detail[0].mail%></span><br> </p> 
            <p class="bio_1"><b>Phone :</b><span><%=about_detail[0].phone%></span><br> </p> 
        </div>
        <div class="col-md-6 mt-3" data-aos="fade-up">
          <h3 class="about_me">SERVICES</h3>
          <hr class="hr">
          <%for(var i=0;i<service.length;i++){%>
            <p><i class="fa-solid fa-clone me-3"></i><%=service[i].service%></p>
          <%}%>
        </div>
        <div class="col-md-6 mt-3" data-aos="fade-up">
          <h3 class="about_me">INTERESTS</h3>
          <hr class="hr">
          <%for(var i=0;i<interest.length;i++){%>
          <p><i class="fa-solid fa-clone me-3"></i><%=interest[i].interest%></p>
          <%}%>       
        </div>
        <div class="col-md-6 mt-5 justify-content-center" data-aos="fade-up">
          <h3 class="about_me justify-content-center">PROGRAMMING</h3>
          <hr class="hr">
          <%for(var i=0;i<programming.length;i++){%>
            <div class="progress w-100 mt-3 rounded-0" role="progressbar" aria-label="Example with label" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
              <div class="progress-bar bg-secondary" style="width:<%=programming[i].percentage%>%"><%=programming[i].t_name%></div>
            </div>
            <%}%>
        </div>
        <div class="col-md-6 mt-5" data-aos="fade-up">
          <h3 class="about_me">LANGUAGE</h3>
          <hr class="hr">
          <ul class="language">
          <li class="sqr"></li>
          <li class="sqr"></li>
          <li class="sqr"></li>
        </ul>
        <ul class="language sec">
          <li class="sqr2">English</li>
          <li class="sqr2">Hindi</li>
          <li class="sqr2">Marathi</li>
        </ul>
        </div>
          </div>
        </div>
    </div>
      </div>
    </div>
  </section>
  <div class="iconss">
    <ul type="none">
      <li><a href="<%=social[0].insta_link%>"><i class="icoo fa-brands fa-instagram"></i></li></a>
      <li><a href="<%=social[0].whatsapp_link%>"><i class="icoo fa-brands fa-whatsapp"></i></li></a>
      <li><a href="<%=social[0].linkdin_link%>"><i class="icoo fa-brands fa-linkedin"></i></li></a>
      <li><a href="<%=social[0].github_link%>"><i class="icoo fa-brands fa-github"></i></li></a>
    </ul>
  
  </div>
  <script src="https://cdn.jsdelivr.net/npm/aos@2.3.4/dist/aos.js"></script>
  <script>
    AOS.init({
        duration: 400,
        offset: 200,
        easing: 'ease-in',
        once: false
    });
  </script>
</html>

